

<h4 class="d-flex justify-content-between mb-4">
<span>Show Post</span>
</h4>


      <div class="card mb-3">
      <div class="card-body">
      <p id="notice"><%= notice %></p>

      <p>
        <strong>Title:</strong>
        <%= @post.title %>
      </p>
      
      <p>
        <strong>Description:</strong>
        <%= @post.description %>
      </p>
      
      <p>
        <strong>Published time:</strong>
        <%= @post.published_time %>
      </p>
      
      <p>
        <strong>Author:</strong>
        <%= @post.author_id %>
      </p>
      
     
      </div>
      </div>
 


<%= link_to 'Edit', edit_post_path(@post) ,class:'btn btn-warning' %> |
<%= link_to 'Back', posts_path,class:'btn btn-secondary' %>
